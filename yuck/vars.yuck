(defvar primary_monitor 1)

(deflisten hyprland :initial `{"monitors": []}` "scripts/hyprland")
(deflisten volume :initial "NaN" "scripts/volume")

(defpoll datetime :interval "1s" "date '+%a %b %-e  %l:%M %p'")
(defpoll wifi-strength :interval "5s" :initial "0" "grep wlan0 /proc/net/wireless | awk '{ print $3 }' | cut -d '.' -f 1")
(defpoll gpu :interval "3s" :initial `{"utilization": 0, "vram_usage": 0}` "scripts/gpu")
